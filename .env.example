# Example environment configuration for AI Music Generation Platform
# Copy to .env and adjust values for your environment

# Runtime / Core
# Valid values: development | staging | production
AIMUSIC_ENVIRONMENT=development
AIMUSIC_LOG_LEVEL=INFO
# Set FAST_DEV to enable dev-only features (dev token endpoint, permissive CORS)
AIMUSIC_FAST_DEV=true
# API network config
AIMUSIC_API_PORT=8000
AIMUSIC_API_WORKERS=4

# Security
# CHANGE THIS IN PRODUCTIO# filepath: /home/beats/Desktop/ai-music-generation/.env
# ... all your other existing settings ...


AIMUSIC_SECRET_KEY=change-me

# Database
AIMUSIC_DATABASE_URL=postgresql+asyncpg://user:password@postgres/aimusicgen

# Redis
AIMUSIC_REDIS_URL=redis://redis:6379/0

# Persistence
AIMUSIC_PERSIST_ENABLED=false

# CORS (comma separated)
AIMUSIC_ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000

# Feature Flags (boolean)
AIMUSIC_ENABLE_BLOCKCHAIN=true
AIMUSIC_ENABLE_COLLABORATION=true
AIMUSIC_ENABLE_MARKETPLACE=true
AIMUSIC_ENABLE_ENTERPRISE=true

# Optional Integrations (placeholders)
AIMUSIC_HUGGINGFACE_TOKEN=
AIMUSIC_STRIPE_SECRET_KEY=
AIMUSIC_STRIPE_PUBLISHABLE_KEY=
AIMUSIC_SENDGRID_API_KEY=
